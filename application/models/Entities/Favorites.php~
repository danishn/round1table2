<?php

namespace Entities;

use Doctrine\ORM\Mapping as ORM;

/**
 * Favorites
 *
 * @ORM\Table(name="favorites")
 * @ORM\Entity
 */
class Favorites
{
    /**
     * @var integer
     *
     * @ORM\Column(name="brand_id", type="integer", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $brandId;

    /**
     * @var string
     *
     * @ORM\Column(name="brand_name", type="string", length=100, nullable=false)
     */
    private $brandName;

    /**
     * @var string
     *
     * @ORM\Column(name="image_url", type="text", length=65535, nullable=false)
     */
    private $imageUrl;

    /**
     * @var string
     *
     * @ORM\Column(name="offer_desc", type="text", length=65535, nullable=false)
     */
    private $offerDesc;

    /**
     * @var string
     *
     * @ORM\Column(name="website_url", type="text", length=65535, nullable=false)
     */
    private $websiteUrl;

    /**
     * @var boolean
     *
     * @ORM\Column(name="status", type="boolean", nullable=false)
     */
    private $status;


    /**
     * Get brandId
     *
     * @return integer
     */
    public function getBrandId()
    {
        return $this->brandId;
    }

    /**
     * Set brandName
     *
     * @param string $brandName
     *
     * @return Favorites
     */
    public function setBrandName($brandName)
    {
        $this->brandName = $brandName;

        return $this;
    }

    /**
     * Get brandName
     *
     * @return string
     */
    public function getBrandName()
    {
        return $this->brandName;
    }

    /**
     * Set imageUrl
     *
     * @param string $imageUrl
     *
     * @return Favorites
     */
    public function setImageUrl($imageUrl)
    {
        $this->imageUrl = $imageUrl;

        return $this;
    }

    /**
     * Get imageUrl
     *
     * @return string
     */
    public function getImageUrl()
    {
        return $this->imageUrl;
    }

    /**
     * Set offerDesc
     *
     * @param string $offerDesc
     *
     * @return Favorites
     */
    public function setOfferDesc($offerDesc)
    {
        $this->offerDesc = $offerDesc;

        return $this;
    }

    /**
     * Get offerDesc
     *
     * @return string
     */
    public function getOfferDesc()
    {
        return $this->offerDesc;
    }

    /**
     * Set websiteUrl
     *
     * @param string $websiteUrl
     *
     * @return Favorites
     */
    public function setWebsiteUrl($websiteUrl)
    {
        $this->websiteUrl = $websiteUrl;

        return $this;
    }

    /**
     * Get websiteUrl
     *
     * @return string
     */
    public function getWebsiteUrl()
    {
        return $this->websiteUrl;
    }

    /**
     * Set status
     *
     * @param boolean $status
     *
     * @return Favorites
     */
    public function setStatus($status)
    {
        $this->status = $status;

        return $this;
    }

    /**
     * Get status
     *
     * @return boolean
     */
    public function getStatus()
    {
        return $this->status;
    }
}

